<div class="container d-flex justify-content-center flex-column ">

  <h1 class="mt-3 d-flex justify-content-center"><strong>Toutes les mouettes</strong></h1>

  <ul>
    <% @mouettes.each do |mouette| %>
    <li><%= link_to mouette.name, mouette_path(mouette)%> </li>
    <% end %>
  </ul>


  <%= link_to "Ajoutez une mouette!", new_mouette_path, class: "btn d-flex justify-content-center align-items-center btn-primary col-3" %>

</div>
