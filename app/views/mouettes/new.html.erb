<h1>Cr√©er votre nouvelle mouette</h1>

<%= simple_form_for @mouette do |f| %>
  <%= f.input :name %>
  <%= f.input :availability %>
  <%= f.input :rating %>
  <%= f.input :latitude %>
  <%= f.input :longitude %>
  <%= f.input :accessories %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit, class: "btn btn-info" %>
<% end %>